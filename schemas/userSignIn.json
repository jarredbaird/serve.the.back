{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/userAuth.json",
  "type": "object",
  "properties": {
    "username": {
      "type": "string",
      "format": "email"
    },
    "password": {
      "type": "string"
    }
  },
  "additionalProperties": false,
  "required": ["username", "password"]
}
